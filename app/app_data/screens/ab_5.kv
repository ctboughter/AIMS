    Image:
        id: screen6_img
        size_hint: 0.65, 0.65
        pos_hint: {'center_x': 0.30, 'center_y': 0.55}
        source: 'app_data/white.png'
        keep_ratio: True

    Button:
        id: clust_butt
        size_hint: 0.15, 0.1
        pos_hint: {'center_x': 0.6, 'center_y': 0.1}
        text: 'Cluster'
        background_down: 'app_data/butt_down.png'
        background_normal: 'app_data/butt_up.png'
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)
        disabled: True
        on_release:
            root.cluster_seqs()
            screen6_img.reload()

    Button:
        size_hint: 0.15, 0.1
        pos_hint: {'center_x': 0.4, 'center_y': 0.1}
        text: 'Reduce Dim'
        background_down: 'app_data/butt_down.png'
        background_normal: 'app_data/butt_up.png'
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)
        on_release:
            root.reduce_dim()
            screen6_img.reload()

    Button:
        id: prev1_5
        size_hint: 0.15, 0.1
