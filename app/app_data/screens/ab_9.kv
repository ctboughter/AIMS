#:kivy 1.8.0
Analysis:
    img9: screen9_img
    next1_9: next1_9
    prop1_sel1: prop1_sel1

    Image:
        source: 'app_data/butt_up.png'
        size_hint: 0.6, 0.075
        pos_hint: {'center_x': 0.50, 'center_y': 0.95}
        keep_ratio: False
        allow_stretch: True

    Label:
        font_name: 'app_data/Poppins-Medium.ttf'
        color: 0.341, 0.392, 1, 1
        size_hint: .1, .1
        pos_hint: {'center_x': 0.95, 'center_y': 0.95}
        text:'A I M S'

    Label:
        size_hint_y: None
        pos_hint: {'center_x': 0.5, 'center_y': 0.95}
        height: '32dp'
        text: 'Step9 - Plotting Clone- and Position-Sensitive Properties'
        font_name: 'app_data/Poppins-Medium.ttf'
        color: 0, 0.033, 0.329, 1
    
    Image:
        id: screen9_img
        size_hint: 0.8, 0.8
        pos_hint: {'center_x': 0.50, 'center_y': 0.475}
        source: 'app_data/white.png'
        keep_ratio: True

    Spinner:
        id: prop1_sel1
        size_hint: 0.15, 0.1
        text: 'Property'
        values: 'Charge','Hydropathy', 'Flexibility', 'Bulk'
        pos_hint: {'center_x': 0.2, 'center_y': 0.8}
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)

    Button:
        size_hint: 0.15, 0.1
        pos_hint: {'center_x': 0.4, 'center_y': 0.8}
        text: 'Choose Color 1'
        background_down: 'app_data/butt_down.png'
        background_normal: 'app_data/butt_up.png'
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)
        on_release:
            root.show_gradWheel1()

    Button:
        size_hint: 0.15, 0.1
        pos_hint: {'center_x': 0.6, 'center_y': 0.8}
        text: 'Choose Color 2'
        background_down: 'app_data/butt_down.png'
        background_normal: 'app_data/butt_up.png'
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)
        on_release:
            root.show_gradWheel2()

    Button:
        size_hint: 0.15, 0.1
        pos_hint: {'center_x': 0.8, 'center_y': 0.8}
        text: 'Default Colors'
        background_down: 'app_data/butt_down.png'
        background_normal: 'app_data/butt_up.png'
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)
        on_release:
            root.reset_gradColor()

    Button:
        size_hint: 0.15, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.1}
        text: 'Get Properties'
        background_down: 'app_data/butt_down.png'
        background_normal: 'app_data/butt_up.png'
        color: (0, 0.033, 0.329, 1)
        border: (0, 0, 0, 0)
        on_release:
            root.get_clone_pos_props()
            screen9_img.reload()

    Button:
        id: prev1_9
        size_hint: 0.15, 0.1
        text: '< Go Back'
        background_normal: 'app_data/transparent.png'
        background_down: 'app_data/transparent.png'
        pos_hint: {'center_x': 0.15, 'center_y': 0.1}
        color: (0.341, 0.392, 1, 1)
        background_color: (0.773, 0.784, 0.796, 1)
        on_release: app.go_prev_screen( num = 8 )

    Button:
        id: next1_9
        size_hint: 0.15, 0.1
        text: 'Next >'
        background_normal: 'app_data/transparent.png'
        background_down: 'app_data/transparent.png'
        pos_hint: {'center_x': 0.85, 'center_y': 0.1}
        color: (0.341, 0.392, 1, 1)
        background_color: (0.773, 0.784, 0.796, 1)
        disabled: True
        on_release: 
            app.go_next_screen( num = 10 )